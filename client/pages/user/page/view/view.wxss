/**menber.wxss**/
.userinfo {
  padding-top: 200px;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.spinner {
  padding-top: 120px;
  display: block;
  margin: 0 auto;
  max-width: 100%;
  width: 150px;
  height: 150px;
}